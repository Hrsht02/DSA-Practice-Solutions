#include <iostream>
using namespace std;

class Node()
{
    int data;
    Node *next;

public:
    Node(int val)
    {
        data = val;
        next = Null;
    }
};

class List()
{
    Node *head;
    Node *tail;

    Public : List()
    {
        head = Null;
        tail = Null;
    }
};

int main()
{
    return 0;
}
